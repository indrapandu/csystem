 SELECT THSystem.[SystemNo] AS 'SystemNo_622',THSystem.[SystemDescription] AS 'SystemDescription_623',THSubSystem.[SubSystemNo] AS 'SubSystemNo_624',THSubSystem.[SubSystemDescription] AS 'SubSystemDescription_625',THDisciplineFolder.[Discipline] AS 'Discipline_626',TETestPackage.[ParentNo] AS 'ParentNo_434',TETestPackage.[ParentDescription] AS 'ParentDescription_435',TETestPackage.[TagNo] AS 'KEYFIELD_436',TETestPackage.[TagDescription] AS 'DESCFIELD_437',TETestPackage.[Area] AS 'Area_673',TDChecksheetPC.[UniqueNo] AS 'KEYFIELD_438',TDChecksheetPC.[ChecksheetNo] AS 'ChecksheetNo_677',TDChecksheetPC.[ChecksheetDescription] AS 'DESCFIELD_439',CONTETestPackage.[ID] as [CONID],CONTETestPackage.[ParentTable] as [CONPARENTTABLE],CONTETestPackage.[ChildTable] as [CONCHILDTABLE],CASE WHEN UPPER(ISNULL(TT_TDChecksheetPC.[StatusOnItem], 'NO')) = 'YES' THEN '' ELSE TTableStatus.[Description] END as 'ConStatus_440', u1.[UserName] AS 'ConnectBy_448',CONTETestPackage.[ConnectedOn] AS 'ConnectedOn_443', u2.[UserName] AS 'StatusBy_441',CONTETestPackage.[StatusOn] AS 'StatusOn_442',CONTETestPackage.[ActualDate] AS 'ActualDate_445',THProject.[ProjectID] AS 'ProjectID_621',TDChecksheetPC.[ProjectID] AS 'ProjectID_446' FROM THProject INNER JOIN ConTHProject ON ConTHProject.[ParentTable] = 'THProject' and ConTHProject.[Parent_ID] = THProject.ID  INNER JOIN THSystem ON ConTHProject.[ChildTable] = 'THSystem' and ConTHProject.[Child_ID] = THSystem.ID  INNER JOIN ConTHSystem ON ConTHSystem.[ParentTable] = 'THSystem' and ConTHSystem.[Parent_ID] = THSystem.ID  INNER JOIN THSubSystem ON ConTHSystem.[ChildTable] = 'THSubSystem' and ConTHSystem.[Child_ID] = THSubSystem.ID  INNER JOIN ConTHSubSystem ON ConTHSubSystem.[ParentTable] = 'THSubSystem' and ConTHSubSystem.[Parent_ID] = THSubSystem.ID  INNER JOIN THDisciplineFolder ON ConTHSubSystem.[ChildTable] = 'THDisciplineFolder' and ConTHSubSystem.[Child_ID] = THDisciplineFolder.ID  INNER JOIN ConTHDisciplineFolder ON ConTHDisciplineFolder.[ParentTable] = 'THDisciplineFolder' and ConTHDisciplineFolder.[Parent_ID] = THDisciplineFolder.ID  INNER JOIN TETestPackage ON ConTHDisciplineFolder.[ChildTable] = 'TETestPackage' and ConTHDisciplineFolder.[Child_ID] = TETestPackage.ID  INNER JOIN ConTETestPackage ON ConTETestPackage.[ParentTable] = 'TETestPackage' and ConTETestPackage.[Parent_ID] = TETestPackage.ID  INNER JOIN TDChecksheetPC ON ConTETestPackage.[ChildTable] = 'TDChecksheetPC' and ConTETestPackage.[Child_ID] = TDChecksheetPC.ID  LEFT OUTER JOIN TTableStatus ON TTableStatus.[StatusCode] = CONTETestPackage.[ConStatus] INNER JOIN TTable [TT_TDChecksheetPC] ON TT_TDChecksheetPC.[ID] = 578 LEFT OUTER JOIN AppUser u1 ON u1.[ID] = CONTETestPackage.[ConnectBy] LEFT OUTER JOIN AppUser u2 ON u2.[ID] = CONTETestPackage.[StatusBy] WHERE THProject.[ProjectRefID] IN ('SCOM') AND THProject.[ID] = 1 AND  TTableStatus.[TTable_ID] = 578 AND TDChecksheetPC.[ProjectID] IN ('SCOM') UNION ALL SELECT THSystem.[SystemNo] AS 'SystemNo_622',THSystem.[SystemDescription] AS 'SystemDescription_623',THSubSystem.[SubSystemNo] AS 'SubSystemNo_624',THSubSystem.[SubSystemDescription] AS 'SubSystemDescription_625',THDisciplineFolder.[Discipline] AS 'Discipline_626',TEPiping.[ParentNo] AS 'ParentNo_434',TEPiping.[ParentDescription] AS 'ParentDescription_435',TEPiping.[TagNo] AS 'KEYFIELD_436',TEPiping.[TagDescription] AS 'DESCFIELD_437',TEPiping.[Area] AS 'Area_673',TDChecksheetPC.[UniqueNo] AS 'KEYFIELD_438',TDChecksheetPC.[ChecksheetNo] AS 'ChecksheetNo_677',TDChecksheetPC.[ChecksheetDescription] AS 'DESCFIELD_439',CONTEPiping.[ID] as [CONID],CONTEPiping.[ParentTable] as [CONPARENTTABLE],CONTEPiping.[ChildTable] as [CONCHILDTABLE],CASE WHEN UPPER(ISNULL(TT_TDChecksheetPC.[StatusOnItem], 'NO')) = 'YES' THEN '' ELSE TTableStatus.[Description] END as 'ConStatus_440', u1.[UserName] AS 'ConnectBy_448',CONTEPiping.[ConnectedOn] AS 'ConnectedOn_443', u2.[UserName] AS 'StatusBy_441',CONTEPiping.[StatusOn] AS 'StatusOn_442',CONTEPiping.[ActualDate]