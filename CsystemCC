 SELECT THSystem.[SystemNo] AS 'SystemNo_613',THSystem.[SystemDescription] AS 'SystemDescription_614',THSubSystem.[SubSystemNo] AS 'SubSystemNo_615',THSubSystem.[SubSystemDescription] AS 'SubSystemDescription_616',THDisciplineFolder.[Discipline] AS 'Discipline_617',TETestPackage.[ParentNo] AS 'ParentNo_412',TETestPackage.[ParentDescription] AS 'ParentDescription_413',TETestPackage.[TagNo] AS 'KEYFIELD_414',TETestPackage.[TagDescription] AS 'DESCFIELD_415',TETestPackage.[Area] AS 'Area_672',TDChecksheetCC.[UniqueNo] AS 'KEYFIELD_675',TDChecksheetCC.[ChecksheetNo] AS 'ChecksheetNo_674',TDChecksheetCC.[ChecksheetDescription] AS 'DESCFIELD_676',CONTETestPackage.[ID] as [CONID],CONTETestPackage.[ParentTable] as [CONPARENTTABLE],CONTETestPackage.[ChildTable] as [CONCHILDTABLE],CASE WHEN UPPER(ISNULL(TT_TDChecksheetCC.[StatusOnItem], 'NO')) = 'YES' THEN '' ELSE TTableStatus.[Description] END as 'ConStatus_421', u1.[UserName] AS 'ConnectBy_423',CONTETestPackage.[ConnectedOn] AS 'ConnectedOn_422', u2.[UserName] AS 'StatusBy_425',CONTETestPackage.[StatusOn] AS 'StatusOn_424',CONTETestPackage.[ActualDate] AS 'ActualDate_426',THProject.[ProjectID] AS 'ProjectID_612',TDChecksheetCC.[ProjectID] AS 'ProjectID_427' FROM THProject INNER JOIN ConTHProject ON ConTHProject.[ParentTable] = 'THProject' and ConTHProject.[Parent_ID] = THProject.ID  INNER JOIN THSystem ON ConTHProject.[ChildTable] = 'THSystem' and ConTHProject.[Child_ID] = THSystem.ID  INNER JOIN ConTHSystem ON ConTHSystem.[ParentTable] = 'THSystem' and ConTHSystem.[Parent_ID] = THSystem.ID  INNER JOIN THSubSystem ON ConTHSystem.[ChildTable] = 'THSubSystem' and ConTHSystem.[Child_ID] = THSubSystem.ID  INNER JOIN ConTHSubSystem ON ConTHSubSystem.[ParentTable] = 'THSubSystem' and ConTHSubSystem.[Parent_ID] = THSubSystem.ID  INNER JOIN THDisciplineFolder ON ConTHSubSystem.[ChildTable] = 'THDisciplineFolder' and ConTHSubSystem.[Child_ID] = THDisciplineFolder.ID  INNER JOIN ConTHDisciplineFolder ON ConTHDisciplineFolder.[ParentTable] = 'THDisciplineFolder' and ConTHDisciplineFolder.[Parent_ID] = THDisciplineFolder.ID  INNER JOIN TETestPackage ON ConTHDisciplineFolder.[ChildTable] = 'TETestPackage' and ConTHDisciplineFolder.[Child_ID] = TETestPackage.ID  INNER JOIN ConTETestPackage ON ConTETestPackage.[ParentTable] = 'TETestPackage' and ConTETestPackage.[Parent_ID] = TETestPackage.ID  INNER JOIN TDChecksheetCC ON ConTETestPackage.[ChildTable] = 'TDChecksheetCC' and ConTETestPackage.[Child_ID] = TDChecksheetCC.ID  LEFT OUTER JOIN TTableStatus ON TTableStatus.[StatusCode] = CONTETestPackage.[ConStatus] INNER JOIN TTable [TT_TDChecksheetCC] ON TT_TDChecksheetCC.[ID] = 573 LEFT OUTER JOIN AppUser u1 ON u1.[ID] = CONTETestPackage.[ConnectBy] LEFT OUTER JOIN AppUser u2 ON u2.[ID] = CONTETestPackage.[StatusBy] WHERE THProject.[ProjectRefID] IN ('SCOM') AND THProject.[ID] = 1 AND  TTableStatus.[TTable_ID] = 573 AND TDChecksheetCC.[ProjectID] IN ('SCOM') UNION ALL SELECT THSystem.[SystemNo] AS 'SystemNo_613',THSystem.[SystemDescription] AS 'SystemDescription_614',THSubSystem.[SubSystemNo] AS 'SubSystemNo_615',THSubSystem.[SubSystemDescription] AS 'SubSystemDescription_616',THDisciplineFolder.[Discipline] AS 'Discipline_617',TEPiping.[ParentNo] AS 'ParentNo_412',TEPiping.[ParentDescription] AS 'ParentDescription_413',TEPiping.[TagNo] AS 'KEYFIELD_414',TEPiping.[TagDescription] AS 'DESCFIELD_415',TEPiping.[Area] AS 'Area_672',TDChecksheetCC.[UniqueNo] AS 'KEYFIELD_675',TDChecksheetCC.[ChecksheetNo] AS 'ChecksheetNo_674',TDChecksheetCC.[ChecksheetDescription] AS 'DESCFIELD_676',CONTEPiping.[ID] as [CONID],CONTEPiping.[ParentTable] as [CONPARENTTABLE],CONTEPiping.[ChildTable] as [CONCHILDTABLE],CASE WHEN UPPER(ISNULL(TT_TDChecksheetCC.[StatusOnItem], 'NO')) = 'YES' THEN '' ELSE TTableStatus.[Description] END as 'ConStatus_421', u1.[UserName] AS 'ConnectBy_423',CONTEPiping.[ConnectedOn] AS 'ConnectedOn_422', u2.[UserName] AS 'StatusBy_425',CONTEPiping.[StatusOn] AS 'StatusOn_424',CONTEPiping.[ActualDate]