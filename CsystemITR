 SELECT THSystem.[SystemNo] AS 'SystemNo_586',THSystem.[SystemDescription] AS 'SystemDescription_587',THSubSystem.[SubSystemNo] AS 'SubSystemNo_588',THSubSystem.[SubSystemDescription] AS 'SubSystemDescription_589',THDisciplineFolder.[Discipline] AS 'Discipline_590',TDChecksheetPC.[ParentNo] AS 'ParentNo_610',TDChecksheetPC.[ParentDescription] AS 'ParentDescription_611',TDChecksheetPC.[TagTypeID] AS 'TagTypeID_605',TDChecksheetPC.[TagType] AS 'TagType_604',TDChecksheetPC.[Area] AS 'Area_721',TDChecksheetPC.[UniqueNo] AS 'KEYFIELD_591',TDChecksheetPC.[ChecksheetNo] AS 'ChecksheetNo_603',TDChecksheetPC.[ChecksheetDescription] AS 'DESCFIELD_592',TDItr.[ITRNo] AS 'KEYFIELD_594',TDItr.[ITRDescription] AS 'DESCFIELD_595',CONTDChecksheetPC.[ID] as [CONID],CONTDChecksheetPC.[ParentTable] as [CONPARENTTABLE],CONTDChecksheetPC.[ChildTable] as [CONCHILDTABLE],CASE WHEN UPPER(ISNULL(TT_TDItr.[StatusOnItem], 'NO')) = 'YES' THEN '' ELSE TTableStatus.[Description] END as 'ConStatus_596', u1.[UserName] AS 'ConnectBy_600',CONTDChecksheetPC.[ConnectedOn] AS 'ConnectedOn_597', u2.[UserName] AS 'StatusBy_601',CONTDChecksheetPC.[StatusOn] AS 'StatusOn_598',CONTDChecksheetPC.[ActualDate] AS 'ActualDate_599',TT2.[TableName] AS 'TABLENAME_653',THProject.[ProjectID] AS 'ProjectID_633',TDItr.[ProjectID] AS 'ProjectID_602' FROM THProject INNER JOIN ConTHProject ON ConTHProject.[ParentTable] = 'THProject' and ConTHProject.[Parent_ID] = THProject.ID  INNER JOIN THSystem ON ConTHProject.[ChildTable] = 'THSystem' and ConTHProject.[Child_ID] = THSystem.ID  INNER JOIN ConTHSystem ON ConTHSystem.[ParentTable] = 'THSystem' and ConTHSystem.[Parent_ID] = THSystem.ID  INNER JOIN THSubSystem ON ConTHSystem.[ChildTable] = 'THSubSystem' and ConTHSystem.[Child_ID] = THSubSystem.ID  INNER JOIN ConTHSubSystem ON ConTHSubSystem.[ParentTable] = 'THSubSystem' and ConTHSubSystem.[Parent_ID] = THSubSystem.ID  INNER JOIN THDisciplineFolder ON ConTHSubSystem.[ChildTable] = 'THDisciplineFolder' and ConTHSubSystem.[Child_ID] = THDisciplineFolder.ID  INNER JOIN ConTHDisciplineFolder ON ConTHDisciplineFolder.[ParentTable] = 'THDisciplineFolder' and ConTHDisciplineFolder.[Parent_ID] = THDisciplineFolder.ID  INNER JOIN TETestPackage ON ConTHDisciplineFolder.[ChildTable] = 'TETestPackage' and ConTHDisciplineFolder.[Child_ID] = TETestPackage.ID  INNER JOIN ConTETestPackage ON ConTETestPackage.[ParentTable] = 'TETestPackage' and ConTETestPackage.[Parent_ID] = TETestPackage.ID  INNER JOIN TEPiping ON ConTETestPackage.[ChildTable] = 'TEPiping' and ConTETestPackage.[Child_ID] = TEPiping.ID  INNER JOIN ConTEPiping ON ConTEPiping.[ParentTable] = 'TEPiping' and ConTEPiping.[Parent_ID] = TEPiping.ID  INNER JOIN TDChecksheetPC ON ConTEPiping.[ChildTable] = 'TDChecksheetPC' and ConTEPiping.[Child_ID] = TDChecksheetPC.ID  INNER JOIN ConTDChecksheetPC ON ConTDChecksheetPC.[ParentTable] = 'TDChecksheetPC' and ConTDChecksheetPC.[Parent_ID] = TDChecksheetPC.ID  INNER JOIN TDItr ON ConTDChecksheetPC.[ChildTable] = 'TDItr' and ConTDChecksheetPC.[Child_ID] = TDItr.ID  LEFT OUTER JOIN TTableStatus ON TTableStatus.[StatusCode] = CONTDChecksheetPC.[ConStatus] INNER JOIN TTable [TT_TDItr] ON TT_TDItr.[ID] = 581 LEFT OUTER JOIN AppUser u1 ON u1.[ID] = CONTDChecksheetPC.[ConnectBy] LEFT OUTER JOIN AppUser u2 ON u2.[ID] = CONTDChecksheetPC.[StatusBy] ,TTable TT2  WHERE THProject.[ProjectRefID] IN ('SCOM') AND THProject.[ID] = 1 AND  TTableStatus.[TTable_ID] = 581 AND  TT2.[TableName] = 'TDChecksheetPC' AND TDItr.[ProjectID] IN ('SCOM') UNION ALL SELECT THSystem.[SystemNo] AS 'SystemNo_586',THSystem.[SystemDescription] AS 'SystemDescription_587',THSubSystem.[SubSystemNo] AS 'SubSystemNo_588',THSubSystem.[SubSystemDescription] AS 'SubSystemDescription_589',THDisciplineFolder.[Discipline] AS 'Discipline_590',TDChecksheetDummy.[ParentNo] AS 'ParentNo_610',TDChecksheetDummy.[ParentDescription] AS 'ParentDescription_611',TDChecksheetDummy.[TagTypeID] AS 'TagTypeID_605',TDChecksheetDummy.[TagType] AS 'TagType_604',TD